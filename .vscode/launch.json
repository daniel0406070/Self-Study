{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/TypeScript/app.ts",
            "outFiles": [
                "${workspaceFolder}/TypeScript/**/*.js"
            ]
        },
        {
            "name": "C/C++: gcc 활성 파일 빌드 및 디버그",
            "type": "cppdbg",
            "request" : "launch",
            "program": "${fileDirname}/${fileBasenameNoExtension}",
            "args": ["<", "/home/daniel/code_space/test_case/input.txt"],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "externalConsole": false,
            "environment": [],
            "MIMode": "gdb",
            "miDebuggerPath": "/usr/bin/gdb",
            "preLaunchTask": "C/C++: gcc 활성 파일 빌드",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
        }
    ]
}